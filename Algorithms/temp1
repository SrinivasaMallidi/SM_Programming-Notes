from django.urls import path, include

from . import views

urlpatterns = [
    path('',views.home,name='common-home'),
    path('gwms/',views.viewGWMS,name='common-view-gwms'),
    path('passwordvault/',views.passwordvault,name='common-passwordvault'),
    path('open_in_app/',views.openServer,name='common-openinapp'),
    path('open/<str:application_name>/', views.open_application, name='open_application'),
    path('checkProdServerDates', views.checkProdServerDates, name='common-checkproddates'),
]
